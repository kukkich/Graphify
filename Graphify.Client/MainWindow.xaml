<reactiveUi:ReactiveWindow 
    x:Class="Graphify.Client.MainWindow"
    x:TypeArguments="viewModel:AppViewModel"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:Graphify.Client"
    xmlns:reactiveUi="http://reactiveui.net"
    xmlns:viewModel="clr-namespace:Graphify.Client.ViewModel"
    Title="Graphify" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBox Padding="5" Height="30" Width="40" Margin="0 30 0 0" HorizontalAlignment="Center" x:Name="ValueBox" />
            <Button x:Name="IncrementButton" Height="30" Width="120" Margin="0 10 0 0">Increment</Button>
        </StackPanel>
    </Grid>
</reactiveUi:ReactiveWindow>
